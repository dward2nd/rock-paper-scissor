<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:motion="http://schemas.android.com/apk/res-auto">

   <Transition
      motion:autoTransition="animateToEnd"
      motion:constraintSetEnd="@+id/end"
      motion:constraintSetStart="@id/start"
      motion:duration="400"
      motion:motionInterpolator="cubic(0.08,0.82,0.34,0.9)">
      <KeyFrameSet></KeyFrameSet>
   </Transition>

   <ConstraintSet android:id="@+id/start">
      <Constraint
         android:id="@+id/view3"
         android:layout_width="0dp"
         android:layout_height="0dp"
         android:alpha="0"
         motion:layout_constraintBottom_toBottomOf="parent"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toTopOf="parent" />
      <Constraint
         android:id="@+id/settingBackBtn"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="48dp"
         android:alpha="0"
         android:scaleX="1.2"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintHorizontal_bias="0.497"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toBottomOf="@+id/imageView40" />
      <Constraint
         android:id="@+id/imageView40"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:alpha="0"
         android:scaleX="1.2"
         motion:layout_constraintBottom_toBottomOf="parent"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintHorizontal_bias="0.495"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toTopOf="parent" />
      <Constraint
         android:id="@+id/textView28"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginStart="24dp"
         android:layout_marginTop="7dp"
         android:alpha="0"
         android:translationX="-20dp"
         motion:layout_constraintStart_toStartOf="@+id/imageView40"
         motion:layout_constraintTop_toTopOf="@+id/settingBgmCheckBox" />
      <Constraint
         android:id="@+id/settingBgmCheckBox"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="30dp"
         android:layout_marginEnd="24dp"
         android:alpha="0"
         android:translationX="20dp"
         motion:layout_constraintEnd_toEndOf="@+id/imageView40"
         motion:layout_constraintTop_toBottomOf="@+id/imageView42" />
      <Constraint
         android:id="@+id/imageView42"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="-38dp"
         android:alpha="0"
         android:scaleX="1.2"
         android:translationY="-20dp"
         motion:layout_constraintEnd_toEndOf="@+id/imageView40"
         motion:layout_constraintStart_toStartOf="@+id/imageView40"
         motion:layout_constraintTop_toTopOf="@+id/imageView40" />
   </ConstraintSet>

   <ConstraintSet android:id="@+id/end"></ConstraintSet>
</MotionScene>