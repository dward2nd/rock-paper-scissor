<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:motion="http://schemas.android.com/apk/res-auto">

   <Transition
      motion:autoTransition="animateToEnd"
      motion:constraintSetEnd="@+id/step2"
      motion:constraintSetStart="@id/step1"
      motion:duration="750"
      motion:staggered="0.4">
      <KeyFrameSet></KeyFrameSet>
   </Transition>

   <ConstraintSet android:id="@+id/step1">
      <Constraint
         android:id="@+id/textView10"
         android:layout_width="0dp"
         android:layout_height="15dp"
         android:alpha="0"
         android:scaleX="1.2"
         android:translationY="10dp"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toTopOf="parent" />
      <Constraint
         android:id="@+id/sessionClientSessionIdBox"
         android:layout_width="0dp"
         android:layout_height="48dp"
         android:layout_marginTop="8dp"
         android:alpha="0"
         android:scaleY="1.2"
         android:translationY="10dp"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toBottomOf="@+id/textView10" />
      <Constraint
         android:id="@+id/textView24"
         android:layout_width="0dp"
         android:layout_height="15dp"
         android:layout_marginTop="32dp"
         android:alpha="0"
         android:scaleY="1.2"
         android:translationY="10dp"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toBottomOf="@+id/sessionClientSessionIdBox" />
      <Constraint
         android:id="@+id/textView25"
         android:layout_width="0dp"
         android:layout_height="15dp"
         android:layout_marginTop="8dp"
         android:alpha="0"
         android:scaleY="1.2"
         android:translationY="10dp"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintHorizontal_bias="0.0"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toBottomOf="@+id/textView24" />
      <Constraint
         android:id="@+id/sessionOpponentSessionIdBox"
         android:layout_width="0dp"
         android:layout_height="48dp"
         android:layout_marginTop="8dp"
         android:alpha="0"
         android:scaleY="1.2"
         android:translationY="10dp"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintHorizontal_bias="0.0"
         motion:layout_constraintStart_toStartOf="parent"
         motion:layout_constraintTop_toBottomOf="@+id/textView25" />
      <Constraint
         android:id="@+id/sessionStartGameBtn"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="16dp"
         android:alpha="0"
         android:scaleY="1.2"
         android:translationY="10dp"
         motion:layout_constraintEnd_toEndOf="parent"
         motion:layout_constraintTop_toBottomOf="@+id/sessionOpponentSessionIdBox" />
   </ConstraintSet>

   <ConstraintSet android:id="@+id/step2"></ConstraintSet>
</MotionScene>