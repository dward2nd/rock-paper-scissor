<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   xmlns:tools="http://schemas.android.com/tools"
   android:layout_width="match_parent"
   android:layout_height="match_parent">

   <ImageView
      android:id="@+id/imageView40"
      android:layout_width="wrap_content"
      android:layout_height="343dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/win" />

   <ImageView
      android:id="@+id/imageView44"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintBottom_toBottomOf="@+id/imageView40"
      app:layout_constraintEnd_toEndOf="@+id/imageView40"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="@+id/imageView40"
      app:layout_constraintTop_toTopOf="@+id/imageView40"
      app:layout_constraintVertical_bias="0.51"
      app:srcCompat="@drawable/exclamation" />

   <TextView
      android:id="@+id/textView30"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="70dp"
      android:fontFamily="@font/gomarice_rockin_record"
      android:text="WIN"
      android:textAlignment="center"
      android:textAppearance="@style/win"
      android:textColor="#FF1616"
      android:textSize="98.48sp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView40"
      app:layout_constraintEnd_toEndOf="@+id/imageView40"
      app:layout_constraintHorizontal_bias="0.49"
      app:layout_constraintStart_toStartOf="@+id/imageView40" />

   <TextView
      android:id="@+id/textView34"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginBottom="12dp"
      android:fontFamily="@font/gomarice_rockin_record"
      android:text="You"
      android:textAlignment="center"
      android:textAppearance="@style/win"
      android:textColor="#FF1616"
      android:textSize="98.48sp"
      app:layout_constraintBottom_toTopOf="@+id/textView30"
      app:layout_constraintEnd_toEndOf="@+id/imageView40"
      app:layout_constraintStart_toStartOf="@+id/imageView40" />
</androidx.constraintlayout.widget.ConstraintLayout>